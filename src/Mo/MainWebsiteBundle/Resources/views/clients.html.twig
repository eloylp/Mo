        <div class="col-md-12">

            <h1>Clientes</h1>

            <ul class="list-unstyled">
                {% for c in clients %}

                    <li>
                        {{ c.name }} - {{ c.lastName }} - {{ c.email }}
                    </li>

                {% endfor %}
            </ul>
        </div>

    </div>

{% endblock %}