<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %} Bienvenido a {{ app_name }} {% endblock %}</title>

    {% block stylesheets %}

        <link rel="stylesheet" href="{{ asset('/assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/assets/vendor/bootstrap/dist/css/bootstrap-theme.css') }}">
        <link rel="stylesheet" href="{{ asset('/assets/css/sticky_footer.css') }}">

    {% endblock %}
</head>

<body>

{% block nav %}
<!-- Fixed navbar -->
<nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ url('mo_main_website_index') }}">{{ app_name }}</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active"><a href="/">Home</a></li>
                <li><a href="{{ url('mo_main_website_products') }}">Productos</a></li>
                <li><a href="{{ url('mo_main_website_clients') }}">Clientes miembros</a></li>
                <li><a href="{{ url('mo_adm_index') }}">Memory Login</a></li>
                <li><a href="{{ url('mo_security_auth') }}">Login</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</nav>

{% endblock %}

{% block body %} {% endblock %}


{% block footer %}
<footer class="footer">
    <div class="container">
        <p class="text-muted">Cont√°ctenos !!! {{ app_email }}</p>
    </div>
</footer>

{% endblock %}

{% block javascript %}

    <script src="{{ asset('/assets/vendor/jquery/dist/jquery.min.js') }}"></script>
    <script src="{{ asset('/assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>

{% endblock %}
</body>
</html>