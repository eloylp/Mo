namespace Mo\MainWebsiteBundle\Entity;


class Bill
{
    private function __construct()
    {
        $this->created_time = new \DateTime();
    }

    private $id;

    protected $bill_num;

    protected $delivery_address;

    protected $billing_address;

    protected $created_time;

    protected $bill_type;

    protected $observations;

}
